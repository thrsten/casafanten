\newcommand{\tikzcasafanten@definecommonstyles}{
	\if@tikzcasafanten@nonsaturated
		\tikzset{
			skin/.style= {draw=\tikzcasafanten@skin!80,top color=\tikzcasafanten@skin!40, bottom color=\tikzcasafanten@skin!80,shading angle=45},
			tridentshape/.style= {color=\tikzcasafanten@tridentshape,fill=\tikzcasafanten@tridentshape},
		}
	\else
		\tikzset{
			skin/.style= {draw=\tikzcasafanten@skin,fill=\tikzcasafanten@skin},
			tridentshape/.style= {color=\tikzcasafanten@tridentshape,fill=\tikzcasafanten@tridentshape},
		}
	\fi
}


